#include <iostream>
#include <math.h>
using namespace std;

int main() {
	float a;
	start:

	cout << "enter a number" << endl;
	cin >> a;

	if (a < 0)
	{
		cout << "error  please don't enter Negative number" << endl;
		goto start;
	}

	cout << sqrt(a) << endl;
	system ("pause");
	//goto start;     //連續輸入
}
